  "stratagems": [
    {
      "name": "Weakness as Strength",
      "id": "343f7652-4c1b-59e6-a468-54feb9525dbc",
      "cost": 1,
      "turn": "either",
      "target": "One HARLEQUINS unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "when": "Your opponent’s Shooting phase or the Fight phase, just after an enemy unit has selected its targets.",
      "effect": "Until the end of the phase, each time an attack targets your unit, if the Strength characteristic of that attack is greater than your unit’s Toughness characteristic, subtract 1 from the Wound roll.",
      "restrictions": "",
      "type": "Battle Tactic",
      "phase": ["fight", "shooting"],
      "fluff": "Flowing through precisely placed steps, the Harlequins first present the appearance of vulnerability before weaving effortlessly around their enemies’ attempts to take advantage of this seeming lapse.",
      "detachment": "Kaleidoscopic Onset",
      "faction_id": "basic"
    },
    {
      "name": "Mocking Flight",
      "id": "8be7d96b-8d6d-5964-baa8-02c7acdc5673",
      "cost": 1,
      "turn": "your",
      "target": "That HARLEQUINS unit.",
      "when": "Your Movement phase, just after a HARLEQUINS unit from your army Falls Back.",
      "effect": "Until the end of the turn, your unit is eligible to shoot and declare a charge in a turn in which it Fell Back.",
      "restrictions": "",
      "type": "Strategic Ploy",
      "phase": ["movement"],
      "fluff": "The Harlequins tumble away in mock terror and panic before spinning back into the fight with cruel laughter and redoubled fury.",
      "detachment": "Kaleidoscopic Onset",
      "faction_id": "basic"
    },
    {
      "name": "Staged Death",
      "id": "1134e9db-134a-5825-88c4-5edd6e1f4ca5",
      "cost": 2,
      "turn": "either",
      "target": "One HARLEQUINS CHARACTER model from your army that was just destroyed. You can use this Stratagem on that model even though it was just destroyed.",
      "when": "Any phase.",
      "effect": "At the end of the phase, set your model back up on the battlefield as close as possible to where it was destroyed and not within Engagement Range of any enemy units, with 2 wounds remaining.",
      "restrictions": "Each model can only be targeted with this Stratagem once per battle.",
      "type": "Strategic Ploy",
      "phase": ["any"],
      "fluff": "With their attention captured by the polychromatic murder raging through their lines, the foe are unprepared for fresh players slipping in from the wings to join the bloody dance.",
      "detachment": "Kaleidoscopic Onset",
      "faction_id": "basic"
    }
  ],
  "enhancements": [
    {
