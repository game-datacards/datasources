    {
      "name": "Servo-Augur Array",
      "id": "27f9ec0b-6d49-5239-b265-7a1d2aa064e3",
      "cost": 1,
      "turn": "your",
      "target": "One ASTRA MILITARUM unit from your army.",
      "when": "End of your Command phase.",
      "effect": "Select one objective marker you control that your unit is within range of. That objective marker remains under your control until your opponent’s Level of Control over that objective marker is greater than yours at the end of a phase.",
      "restrictions": "",
      "type": "Strategic Ploy",
      "phase": ["command"],
      "fluff": "Equipped to far higher standards than front-line regiments, the Militarum Tempestus can deploy servitor-controlled augur arrays and comms relay modules to secure and remotely monitor vital ground, enabling a swift advance on the next target.",
      "detachment": "Strike Platoon Gorgonne VI",
      "faction_id": "basic"
    },
    {
      "name": "Swift Execution",
      "id": "7dcc8eae-bc2a-5424-9da6-7f0b6099fce1",
      "cost": 1,
      "turn": "your",
      "target": "One ASTRA MILITARUM INFANTRY unit from your army that has not been selected to move this phase.",
      "when": "Your Movement phase.",
      "effect": "Until the end of the phase, add 3\" to the Move characteristic of models in your unit.",
      "restrictions": "",
      "type": "Strategic Ploy",
      "phase": ["movement"],
      "fluff": "The violent potential of Tempestus Scions is enhanced by a brutal training regimen and rigid discipline, such that the most hazardous or inhumane orders of their superiors are carried out swiftly and mercilessly.",
      "detachment": "Strike Platoon Gorgonne VI",
      "faction_id": "basic"
    },
    {
      "name": "Indoctrinated Obedience",
      "id": "b5591578-06d9-583a-b2d2-dac838093f16",
      "cost": 1,
      "turn": "opponents",
      "target": "One ASTRA MILITARUM unit from your army that was selected as the target of one or more of the attacking unit’s attacks.",
      "when": "Your opponent’s Shooting phase, just after an enemy unit has selected its targets.",
      "effect": "Until the end of the phase, each time a model in your unit is destroyed, if that model has not shot this phase, roll one D6, adding 3 if your unit has the VEHICLE keyword. On a 3+, do not remove the destroyed model from play; it can shoot after the attacking model’s unit has finished making its attacks and is then removed from play.",
      "restrictions": "",
      "type": "Strategic Ploy",
      "phase": ["shooting"],
      "fluff": "In the mind of every Tempestus Scion, duty overrides any instinct for self-preservation. Even as they fall, they are conditioned to draw upon their last reserve of strength to see the foe slain and the mission completed.",
      "detachment": "Strike Platoon Gorgonne VI",
      "faction_id": "basic"
    }
